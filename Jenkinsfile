pipeline {
  agent any
  stages {
    stage('build') {
      steps {
        echo "This is Build Stage"
      }
     } 
    stage('Deploy_to_dev') {
      steps {
        echo "This is Deploy to dev Stage"
      }
     } 
    stage('Deploy_to_Prod') {
      steps {
        echo "This is Deploy to Prod Stage"
      }  
    }
   } 
}
